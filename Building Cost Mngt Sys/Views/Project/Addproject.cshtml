@model Building_Cost_Mngt_Sys.Models.Project.ProjectViewModel;
<form asp-action="Addproject" asp-controller="Project" >
  <div class="form-group">
    <label for="exampleInputEmail1">Project Name </label>
        <input type="text" asp-for="ProjectName" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter Project Name" required>
    <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
  </div>
  <div class="form-group">
        <label for="exampleInputPassword1">Address</label>
        <input type="text" asp-for="Address" class="form-control" id="exampleInputPassword1" placeholder="Enter Address" required>
  </div>

    <div class="form-group">
        <label for="exampleInputPassword1">no_of_floors</label>
        <input type="number" asp-for="no_of_floors" class="form-control" id="exampleInputPassword1" placeholder="Enter no_of_floors" required>
    </div>

    <div class="form-group">
        <label for="exampleInputPassword1">no_of_house_per_floor</label>
        <input type="number" asp-for="no_of_house_per_floor" class="form-control" id="exampleInputPassword1" placeholder="Enter no_of_house_per_floor" required>
    </div>

  <div class="form-group form-check">
    
    <button type="submit" class="btn btn-primary">Submit</button>
  </div>
</form>
